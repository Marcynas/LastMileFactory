{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "LastMileFactory"
		},
		"AzurePostgreSql_Products_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzurePostgreSql_Products'"
		},
		"Formating notebook_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Formating notebook'"
		},
		"iki_ProductsWh_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'iki_ProductsWh'"
		},
		"iki_products_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'iki_products'"
		},
		"Formating notebook_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0914-123506-hbz51gu1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "LastMileFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzurePostgreSql_Products_connectionString": {
						"value": "[parameters('AzurePostgreSql_Products_connectionString')]"
					},
					"Formating notebook_accessToken": {
						"value": "[parameters('Formating notebook_accessToken')]"
					},
					"iki_ProductsWh_connectionString": {
						"value": "[parameters('iki_ProductsWh_connectionString')]"
					},
					"iki_products_connectionString": {
						"value": "[parameters('iki_products_connectionString')]"
					},
					"Formating notebook_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Formating notebook_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}