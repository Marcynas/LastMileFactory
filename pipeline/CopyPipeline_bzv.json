{
	"name": "CopyPipeline_bzv",
	"properties": {
		"activities": [
			{
				"name": "WhToClean",
				"description": "Move important stuff data to \"ikiProductsClean\" database",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "public.rawIkiData"
					},
					{
						"name": "Destination",
						"value": "public.cleanIkiData"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzurePostgreSqlSource"
					},
					"sink": {
						"type": "AzurePostgreSQLSink",
						"writeBatchSize": 1000000,
						"writeBatchTimeout": "00:30:00"
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_bzv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_bzv",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}